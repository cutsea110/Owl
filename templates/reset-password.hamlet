<!-- Reset password on onetime login -->
<div .page-header>
  <h2>_{MsgResetPasswordOnOnetimeLogin}

<div .tab-content>
  $maybe msg <- mmsg
    <div .alert.alert-block>
      <button type="button" .close data-dismiss="alert">&times;
      <p>#{msg}

<form method=post action=@{HELP ResetPasswordR} enctype=#{e}>
  ^{w}
  <div .control-group>
    <div .controls.btn-group>
      <button type="submit" .btn.btn-primary> _{MsgResetPassword}
